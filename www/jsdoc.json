{
    "opts": {
        "template": "node_modules/better-docs"
      },
    "source": {
        "include": ["./src/js/modules/", "./src/js/", "./README.md", "./package.json"],
        "exclude": [ /* array of paths to exclude */ ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
        },
    "plugins": [
        "node_modules/better-docs/category"
    ]
}