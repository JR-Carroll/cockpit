{
    "opts": {
        "template": "node_modules/toast-jsdoc",
        "whitelist": ["Bookshelf", "Model", "Collection", "Events", "API"]
      },
    "source": {
        "include": ["./index.js", "./SystemResourceMonitor.js", "./README.md", "./package.json"],
        "exclude": [ /* array of paths to exclude */ ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
        },
    "plugins": [
        "node_modules/better-docs/category"
    ]
}